{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ameer-proj/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/ameer-proj/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/ameer-proj",
  "projects": {
    "ameer-proj": {
      "type": "application",
      "root": "apps/ameer-proj",
      "entryFile": "main",
      "sourceRoot": "apps/ameer-proj/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ameer-proj/tsconfig.app.json"
      }
    },
    "b": {
      "type": "application",
      "root": "apps/b",
      "entryFile": "main",
      "sourceRoot": "apps/b/src",
      "compilerOptions": {
        "tsConfigPath": "apps/b/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}